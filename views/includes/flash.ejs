<% if(success && success.length){%>
    <div
  class="mt-4 alert alert-success alert-dismissible fade show col-8 offset-2"
  role="alert"
>
 <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>

<% if(failure && failure.length){%>
    <div
  class="mt-4 alert alert-danger alert-dismissible fade show col-8 offset-2"
  role="alert"
>
 <%= failure %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>
